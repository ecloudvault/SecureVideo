<%@tag description="guest pages layout" pageEncoding="UTF-8" isELIgnored="false" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="s"  uri="http://www.springframework.org/security/tags"%><%@attribute name="content" fragment="true" %><html><head>    <meta charset="UTF-8">    <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport' />    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>    <title>Secure Video</title>    <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>    <!-- Bootstrap 3.3.2 -->    <link href="<c:url value='/static/lib/bootstrap/dist/css/bootstrap.min.css'/>" rel="stylesheet" type="text/css" />    <!-- Font Awesome Icons -->    <link href="<c:url value='/static/lib/font-awesome/css/font-awesome.min.css'/>" rel="stylesheet" type="text/css" />    <!-- Theme style -->    <link href="<c:url value='/static/lib/admin-lte/dist/css/AdminLTE.min.css'/>" rel="stylesheet" type="text/css" />    <!-- iCheck -->    <link href="<c:url value='/static/lib/iCheck/square/blue.css'/>" rel="stylesheet" type="text/css" />    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->    <!--[if lt IE 9]>    <script src="<c:url value='/static/lib/html5shiv.min.js'/>"></script>    <script src="<c:url value='/static/lib/respond.min.js'/>"></script>    <![endif]--></head><body class="login-page"><input type="hidden" id="_currentNamespace" name="_currentNamespace" value="${requestScope._currentTenant.namespace}" /><div class="login-box">    <div class="login-logo">        <a href="#"><b>SecureVideo</a>    </div><!-- /.login-logo -->    <div class="login-box-body">        <jsp:invoke fragment="content"/>    </div><!-- /.login-box-body --></div><!-- /.login-box --><!-- jQuery 2.1.3 --><script src="<c:url value='/static/lib/jquery/dist/jquery.min.js'/>"></script><!-- Bootstrap 3.3.2 JS --><script src="<c:url value='/static/lib/bootstrap/dist/js/bootstrap.min.js'/>" type="text/javascript"></script><!-- iCheck --><script src="<c:url value='/static/lib/iCheck/icheck.min.js'/>" type="text/javascript"></script><script>    $(function () {        $('input').iCheck({            checkboxClass: 'icheckbox_square-blue',            radioClass: 'iradio_square-blue',            increaseArea: '20%' // optional        });    });</script></body></html>